{
  "Dhcp6": {
    "interfaces-config": {
      "interfaces": [ "{{ lan_iface }}" ]
    },
    "subnet6": [
      {
        "subnet": "2001:db8:0:1::/64",
        "pools": [
          { "pool": "2001:db8:0:1::10 - 2001:db8:0:1::50" }
        ],
        "interface": "{{ lan_iface }}",
        "option-data": [
          {
            "name": "dns-servers",
            "data": "2001:db8:0:1::1"
          }
        ]
      }
    ],
    "lease-database": {
      "type": "memfile",
      "persist": true
    },
    "loggers": [
    {
        "name": "kea-dhcp6",
        "output_options": [
        {
            "output": "stdout"
        }
        ],
        "severity": "DEBUG",
        "debuglevel": 99
    }
    ]
  }
}
